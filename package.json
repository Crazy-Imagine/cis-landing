{
	"name": "cis-landing",
	"version": "1.0.0",
	"author": "davidadc",
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "astro dev",
		"start": "astro dev --remote",
		"build": "astro check && astro build --remote",
		"preview": "astro preview",
		"astro": "astro",
		"check": "astro check && tsc --noEmit",
		"lint": "eslint --ext .astro,.ts,.js,.tsx . --ignore-pattern \"src/components/external-scripts/*\" --ignore-pattern \"src/utils/staff-augmentation-info.ts\" --ignore-pattern \"src/consts/ourProcess.ts\"",
		"lint:fix": "eslint --ext .astro,.ts,.js,.tsx . --ignore-pattern \"src/components/external-scripts/*\" --ignore-pattern \"src/utils/staff-augmentation-info.ts\" --ignore-pattern \"src/consts/ourProcess.ts\" --fix",
		"format": "prettier --write src",
		"format:check": "prettier --check src",
		"prepare": "husky",
		"static-build": "serve ./dist",
		"hybrid-build": "node ./dist/server/entry.mjs"
	},
	"engines": {
		"node": "18.20.2"
	},
	"dependencies": {
		"@astrojs/check": "0.7.0",
		"@astrojs/node": "^8.3.4",
		"@astrojs/react": "3.4.0",
		"@astrojs/sitemap": "3.1.4",
		"@astrojs/tailwind": "5.1.0",
		"@fontsource/roboto": "5.0.13",
		"@hookform/resolvers": "3.4.2",
		"@nanostores/react": "0.7.2",
		"@tailwindcss/forms": "0.5.7",
		"astro": "4.16.7",
		"axios": "1.7.7",
		"marked": "12.0.2",
		"nanostores": "0.10.3",
		"qs": "6.12.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-hook-form": "7.51.5",
		"sharp": "0.33.4",
		"sweetalert2": "11.14.5",
		"swiper": "11.1.3",
		"yup": "1.4.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "0.43.1",
		"@types/qs": "6.9.15",
		"@types/react": "18.3.2",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/parser": "7.9.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-astro": "1.2.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"husky": "9.0.11",
		"lightningcss": "1.25.0",
		"lint-staged": "15.2.2",
		"postcss-import": "16.1.0",
		"postcss-nesting": "12.1.4",
		"prettier": "3.2.5",
		"prettier-plugin-astro": "0.13.0",
		"prettier-plugin-astro-organize-imports": "0.4.8",
		"prettier-plugin-tailwindcss": "0.5.14",
		"tailwindcss": "3.4.3",
		"typescript": "5.4.5"
	},
	"packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
